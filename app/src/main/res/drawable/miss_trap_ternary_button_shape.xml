<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/miss_trap_ternary_button">
        <shape android:shape="rectangle">
            <solid android:color="@color/miss"/>
            <stroke android:color="@color/dark" android:width="2dp"/>
            <size android:width="@dimen/CircleTrapButton" android:height="@dimen/CircleTrapButton"/>
            <corners android:radius="4dp" />
        </shape>
    </item>
    <item
        android:width="@dimen/CircleTrapButtonIcon"
        android:height="@dimen/CircleTrapButtonIcon"
        android:gravity="center">
        <bitmap
            android:gravity="fill_horizontal|fill_vertical"
            android:src="@drawable/miss_icon"/>
    </item>
</layer-list>